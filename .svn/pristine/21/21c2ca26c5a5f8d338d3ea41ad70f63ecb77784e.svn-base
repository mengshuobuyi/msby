#import <UIKit/UIKit.h>
#import "QWBaseVC.h"

@interface DiseaseDetailViewController : QWBaseVC

/**
 *  @brief 调用本界面,必须传入一个diseaseId
 */
@property (nonatomic,copy)NSString* diseaseId;

/**
 *  @brief 判断是否从常见疾病过来,用来区分是否需要收藏功能
 *
 *  @p 如果不是,则不需要赋值,有收藏功能;如果是,则传入comeFromNormalDisease = YES,有收藏功能;
 */
@property (nonatomic ,assign) BOOL comeFromNormalDisease;

@end
